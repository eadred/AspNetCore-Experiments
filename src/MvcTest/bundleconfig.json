[
  {
    "outputFileName": "wwwroot/css/site.css",
    "inputFiles": [
      "content/css/site/**/*.css"
    ]
  },
  {
    "outputFileName": "wwwroot/css/suites.css",
    "inputFiles": [
      "content/css/suites/**/*.css"
    ]
  },


  {
    "outputFileName": "temp/ts-out/site.js",
    "inputFiles": [
      "content/ts/site/**/*.ts"
    ]
  },
  {
    "outputFileName": "wwwroot/js/site.js",
    "inputFiles": [
      "temp/ts-out/site.js",
      "content/js/site/**/*.js"
    ],
    "minify": {
      "enabled": false,
      "renameLocals": false
    },
    "sourceMap": false
  },


  {
    "outputFileName": "wwwroot/js/suites.js",
    "inputFiles": [
      "content/js/suites/suites.module.js",
      "content/js/suites/**/*.js"
    ],
    "minify": {
      "enabled": false,
      "renameLocals": false
    },
    "sourceMap": false
  },


  {
    "outputFileName": "temp/ts-out/tstest.js",
    "inputFiles": [
      "content/ts/tstest/**/*.ts"
    ]
  },
  {
    "outputFileName": "wwwroot/js/tstest.js",
    "inputFiles": [
      "temp/ts-out/tstest.js",
      "content/js/tstest/**/*.js"
    ],
    "minify": {
      "enabled": false,
      "renameLocals": false
    },
    "sourceMap": false
  },


  {
    "outputFileName": "wwwroot/templates/dialogs/error.html",
    "inputFiles": [ "content/js/site/dialogs/error/errorDialog.tpl.html" ]
  },
  {
    "outputFileName": "wwwroot/templates/dialogs/confirm.html",
    "inputFiles": [ "content/js/site/dialogs/confirm/confirmDialog.tpl.html" ]
  }
]